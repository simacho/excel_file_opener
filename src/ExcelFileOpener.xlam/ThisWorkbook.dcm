VERSION 1.0 CLASS
BEGIN
  MultiUse = -1  'True
END
Attribute VB_Name = "ThisWorkbook"
Attribute VB_GlobalNameSpace = False
Attribute VB_Creatable = False
Attribute VB_PredeclaredId = True
Attribute VB_Exposed = True

' äÑÇËìñÇƒ
Private Sub Workbook_Open()
    Application.OnKey "^A", "OpenFile_ACTIVE_PATH"  ' [Ctrl]+[A]
    Application.OnKey "^R", "OpenFile_RECURSIVE_PATH"  ' [Ctrl]+[R]
    Application.OnKey "^M", "OpenFile_RECENT_FILE"  ' [Ctrl]+[M]
    Application.OnKey "^S", "OpenFile_SWITCH_BOOK"  ' [Ctrl]+[S]
End Sub
 
' äÑÇËìñÇƒâèú
Private Sub Workbook_BeforeClose(Cancel As Boolean)
    Application.OnKey "^A"   ' [Ctrl]+[A]
    Application.OnKey "^R"   ' [Ctrl]+[R]
    Application.OnKey "^M"   ' [Ctrl]+[M]
    Application.OnKey "^S"   ' [Ctrl]+[S]
End Sub
